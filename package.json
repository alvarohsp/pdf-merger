{
  "name": "pdf-merger",
  "productName": "PDF Merger",
  "version": "0.5.1-beta",
  "description": "Simple PDF utility tool",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --extensions \".ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "npm run build && electron-forge start",
